CREATE TABLE candidateScraped (
	regNo integer   PRIMARY KEY ,
	hackerrankStar int,
	codechefStar int,
	codechefRating int
);

alter table candidateScraped add constraint connec foreign key(regNo) references candidateDetails(regno);


//this is just an example to see if it works P.s.- it does lol
insert into candidateScraped values(1,2,3,4);

select*from candidateScraped;